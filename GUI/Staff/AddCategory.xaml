<Window x:Class="GUI.Staff.AddCategory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.Staff"
        mc:Ignorable="d"
        Title="AddCategory" Height="88" Width="239">
    <Grid Background="DarkGray" Margin="0,0,-5,-6">
        <StackPanel Margin="5,0,7,7">
            <TextBlock Text="Ny kategori:"></TextBlock>
            <TextBox x:Name="tb_NewCategory"></TextBox>
            <Button x:Name="AddCategoryButton" Content="Lägg till" Click="AddCategoryButton_Click" Height="33" Width="226"/>
        </StackPanel>
    </Grid>
</Window>
